ifQ(7651)
.answered(1)
.andQ(7651)
.answered(2)
.andQ(7651)
.answered(3)
.setValue(Q(7651),1,2,3,4);

// If QID 7651 is not answered Option 4, then set value to Option 6 to QID 7651.
ifQ(7651)
.notAnswered(4)
.setValue(Q(7651), 6)

