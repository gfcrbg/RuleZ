.answered(1)
.andQ(5881)
.answered(1)
.forceAppear(Q(3391), Q(4881), Q(4891))
.forceDisappear(Q(3371), Q(3361), Q(5741), Q(3381), Q(3401), Q(5431))
.setValue(Q(3371), 3)
.setValue(Q(3361), 3)
.setValue(Q(5741), 6)
.setValue(Q(3381), 3)
.setValue(Q(3401), 3)
.setValue(Q(5431), 3);

