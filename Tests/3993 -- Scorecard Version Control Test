			<script type='text/javascript' language='javascript'>
				document.domain='sassieshop.com';
				function addQuestion(SurveyID)
				{
					var QFmt = window.document.AddQuestion.QFmt.options[window.document.AddQuestion.QFmt.selectedIndex].value;
					var QID = window.document.AddQuestion.QOrder.options[window.document.AddQuestion.QOrder.selectedIndex].value;

					if(QFmt != 0)
					{
						var newWindow = window.open('QuestionAdmin.php?QFmt='+QFmt+'&QID='+QID+'&SurveyID='+SurveyID+'&mode=add','MiniWindow','width=730,height=650,menubar=0,scrollbars=yes,resizable=yes');
						newWindow.focus();
					}
					else
						alert ("You must choose a question format");
				}

				function editQuestion(QID,SurveyID)
				{
					var newWindow = window.open('QuestionAdmin.php?SurveyID='+SurveyID+'&mode=view&Qno='+QID,'MiniWindow','width=740,height=650,menubar=0,scrollbars=yes,resizable=yes');
					newWindow.focus();
				}

				function viewQuestionVisibility(QID,SurveyID)
				{
					var newWindow = window.open('QuestionAdmin.php?SurveyID='+SurveyID+'&mode=showhideEdit&Qno='+QID+'&showhideLevel=6','MiniWindow','width=740,height=650,menubar=0,scrollbars=yes,resizable=yes');
					newWindow.focus();
				}

				function viewLocationVisibility(SurveyID)
				{
					var levelFilter = window.document.locVisible.levelFilter.options[window.document.locVisible.levelFilter.selectedIndex].value;
					if (levelFilter.split('^')[1] == '6')
					{
						alert('Please choose a level');
						return;
					}

					var newWindow = window.open('QuestionAdmin.php?SurveyID='+SurveyID+'&mode=showhideLevel&levelFilter='+levelFilter,'MiniWindow','width=740,height=650,menubar=0,scrollbars=yes,resizable=yes');
					newWindow.focus();
				}

				function makeGrid(SurveyID)
				{
					var gridStart = '';
					var gridPos = '';
					var resume = '';

					if(window.document.gridMaker.mode.value == "edit")
						gridStart = window.document.gridMaker.gridStart.options[window.document.gridMaker.gridStart.selectedIndex].value;
					else
					{
						if(window.document.gridMaker.mode.value == "add")
							gridPos = window.document.gridMaker.QOrder.options[window.document.gridMaker.QOrder.selectedIndex].value;
						else
						{
							if(window.document.gridMaker.mode.value == "resume")
							{
								gridPos = 1;
								var resume = 1;
							}
						}
					}
					var gridWindow = window.open('QuestionAdmin.php?SurveyID='+SurveyID+'&mode=makeGrid&startQno='+gridStart+'&startPos='+gridPos+'&resume='+resume,'MiniWindow','width=600,height=500,menubar=0,scrollbars=yes,resizable=yes');
					gridWindow.focus();
				}

				function makeNewGrid(SurveyID)
				{
					var gridStart = '';
					var gridPos = '';
					var resume = '';

					if(window.document.gridMaker.mode.value == "edit")
						gridStart = window.document.gridMaker.gridStart.options[window.document.gridMaker.gridStart.selectedIndex].value;
					else
					{
						if(window.document.gridMaker.mode.value == "add")
							gridPos = window.document.gridMaker.QOrder.options[window.document.gridMaker.QOrder.selectedIndex].value;
						else
						{
							if(window.document.gridMaker.mode.value == "resume")
							{
								gridPos = 1;
								var resume = 1;
							}
						}
					}

					var gridWindow = window.open('QuestionAdmin.php?newGrid=1&SurveyID='+SurveyID+'&mode=makeGrid&startQno='+gridStart+'&startPos='+gridPos+'&resume='+resume,'MiniWindow','width=900,height=700,left=85,top=10,menubar=0,scrollbars=yes,resizable=yes');
					gridWindow.focus();
				}

				function previewQuestion(QID, SurveyID)
				{
					var newWindow = window.open('QuestionPreview.php?Qno='+QID+'&SurveyID='+SurveyID,'MiniWindow','width=700,height=650,menubar=0,scrollbars=yes,resizable=yes');
					newWindow.focus();
				}

				function verifyDelete(QID, isActive)
				{
					var del = 0;

					if(!isActive)
					{
						del = confirm ("Are you sure you want to delete question ID "+QID+"?");
					}
					else
					{
						del = confirm ("WARNING: This form has active shops.\nDeleting will cause all data for this question to be permanently lost.\nAre you sure you want to delete question ID "+QID+"?");
					}
					return (del);
				}

				function verifyNormativeDelete(QID, isActive)
				{
					return confirm ("WARNING: Deleting this question will permanently delete it and its responses from all surveys.\nAre you sure you want to delete question ID "+QID+"?");
				}

				// Code copied from SurveyAdmin
				var request = {
					table:false,

					/***
					create/init the wonderfully magical iframe
					//*/
					startFrame:function(surveyID, customizationID)
					{
						srcpage = 'https://www.sassieshop.com/2mysteryshopinc/admin/getSurveysByCustomizationID.php?customizationID='+customizationID+'&surveyID='+surveyID+'&domain=sassieshop.com';

						// set up a new frame if we don't have one.
						if(request.table == false)
						{
							var iframe;
							iframe = document.createElement('IFRAME');

							with(iframe)
							{
								width = '45%';
								height = '300px';
								src = 'about:blank';
								frameborder='no';
								border='0';
							}

							document.body.appendChild(iframe);

							$(iframe).css(
							{
								position:'absolute',
								top:'10px',
								right:'15px'
							});

							//get page scroll offset
							iframe.getPageY=function()
							{
								//we dont know what the best function is yet
								//so when we find out, override this function for next use.
								if(typeof window.pageYOffset != 'undefined' && !isNaN(parseInt(window.pageYOffset,10)))
								{
									this.getPageY = function()
									{
										return parseInt(window.pageYOffset,10);
									}
								}
								else if(!isNaN(parseInt(document.body.scrollTop,10)))
								{
									this.getPageY = function()
									{
										return parseInt(document.body.scrollTop,10);
									}
								}
								else
								{
									this.getPageY = function()
									{
										return false;
									}
								}

								return this.getPageY();
							}

							iframe.fixed = function()
							{
								this.style.top= (this.getPageY()+50)+'px';
							}

							request.table = iframe;
							request.startFrame(surveyID, customizationID);
						}
						else
						{
							//rethreading help IE? <- YES
							setTimeout("$(request.table).css({display:''})",255);
							setTimeout("request.table.src = '"+srcpage+"';",250);
						}

						request.table.interval = setInterval('if(typeof request.table.fixed == \'function\') { request.table.fixed() } else { clearInterval(request.table.interval) }',500);
					},

					closeFrame:function()
					{
						$('iframe').hide();
					}
				};
		
				$(document).ready(function()
				{
					var langPref = (typeof(DATE_PICKER_LANG_PREF) == 'undefined' ? 'en' : DATE_PICKER_LANG_PREF);
					var dateFormat = (0 ? "d/m/yy" : "m/d/yy");

					var defaults = {
									changeMonth: true,
									changeYear: true,
									dateFormat: dateFormat,
									// need to update both alt fields, posted input & client side input (used by rulez etc)
									altFormat: "yy-m-d",
									showOn: "both",
									buttonImage: "/sassiehelpers/common/images/icon-calendar_20j.png",
									buttonImageOnly: true
							};
					var opts = $.extend({}, $.datepicker.regional[langPref], defaults);

					$( "#date1" ).datepicker(opts);
					$( "#date2" ).datepicker(opts);

					dimDates = function() {
						if (this.value == 'm/d/yyyy' || this.value == 'd/m/yyyy')
							this.style.color = '#C0C0C0';
						else
							this.style.color = '#000000';
					}

					$('#date1').change(dimDates);
					$('#date2').change(dimDates);
					$('#date1').change();
					$('#date2').change();
				});
			</script></head>

